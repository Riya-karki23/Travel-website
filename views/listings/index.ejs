<% layout("/layouts/boilerplate")%>
<body>

<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
    <%for(let list of allListing)
    {%>
        <a href="/listings/<%=list._id%>" class="list-link">
    <div class="card col" >
        <img src="<%=list.image%>" class="card-img-top" alt="list_image" style="height:20rem"/>
        <div class="card-body">
          <p class="card-text">
           <b> <%=list.title%></b>
           
           
        </div>
      </div>
      </a>
      <%}%>
</div>

</body>
